{
  "name": "wanderlink",
  "version": "1.0.0",
  "private": true,
  "description": "WanderLink - Web3 Social Travel Platform with AI Agents",
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:contracts": "cd contracts && npx hardhat node",
    "build:frontend": "cd frontend && npm run build",
    "build:contracts": "cd contracts && npx hardhat compile",
    "test:contracts": "cd contracts && npm test",
    "deploy:hedera": "cd contracts && npm run deploy:hedera",
    "deploy:polygon": "cd contracts && npm run deploy:polygon",
    "lint": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "web3",
    "travel",
    "ai-agents",
    "hedera",
    "polygon",
    "ethereum",
    "decentralized",
    "social-travel"
  ],
  "author": "WanderLink Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  }
}
